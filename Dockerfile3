RUN aws codeartifact get-package-version-asset \
    --domain assyvajrala \
    --domain-owner 688567283917 \
    --repository marvel \
    --package com.assy.com/cluster_jar \
    --package-version 1.0-20250305.103220-1 \
    --asset cluster_jar-1.0-20250305.103220-1.jar \
    --output /app/app.jar

# Verify the JAR file was downloaded successfully
RUN ls -l /app/
RUN file /app/app.jar 
